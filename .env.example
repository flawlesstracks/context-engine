ANTHROPIC_API_KEY=your-key-here

# Google OAuth (required for /auth/google login)
# Create credentials at https://console.cloud.google.com/apis/credentials
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
GOOGLE_CLIENT_SECRET=your-google-client-secret

# Session secret — random string for signing JWTs and encrypting connector tokens
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
SESSION_SECRET=replace-with-random-hex-string

# ShareFile OAuth (required for /api/connect/sharefile)
# Create app at ShareFile admin → Developer Settings
SHAREFILE_CLIENT_ID=your-sharefile-client-id
SHAREFILE_CLIENT_SECRET=your-sharefile-client-secret
SHAREFILE_SUBDOMAIN=your-subdomain

# Clio OAuth (pending developer approval)
# CLIO_CLIENT_ID=your-clio-client-id
# CLIO_CLIENT_SECRET=your-clio-client-secret
